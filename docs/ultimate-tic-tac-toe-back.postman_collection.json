		{
			"name": "Flows - Test Suites",
			"item": [
				{
					"name": "01 Core Gameplay",
					"description": "SignalR note: connect to /move-updates-hub and JoinGame(gameId) to receive MoveApplied/MoveRejected events.",
					"item": [
						{
							"name": "StartGame (flow)",
							"request": {
								"method": "POST",
								"header": [
									{
										"key": "Content-Type",
										"value": "application/json"
									}
								],
								"url": "{{baseUrl}}/api/game/start"
							},
							"event": [
								{
									"listen": "test",
									"script": {
										"type": "text/javascript",
										"exec": [
											"const data = pm.response.json();",
											"pm.test(\"status 200\", () => {",
											"  pm.response.to.have.status(200);",
											"});",
											"pm.test(\"start game payload\", () => {",
											"  pm.expect(data).to.have.property(\"isSuccess\");",
											"  pm.expect(data).to.have.property(\"code\");",
											"  pm.expect(data).to.have.property(\"value\");",
											"  pm.expect(data.isSuccess).to.be.true;",
											"  pm.expect(data.value).to.have.property(\"gameId\");",
											"  pm.expect(data.value).to.have.property(\"playerXId\");",
											"  pm.expect(data.value).to.have.property(\"playerOId\");",
											"});",
